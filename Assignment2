## A pair of functions that cache the inverse of a matrix


## Creates a special matrix object that can cache its inverse
makeCacheMatrix <- function( m = matrix() ) {
  

  i <- NULL

  set <- function( matrix ) {
    m <<- matrix
    i <<- NULL
  }
  

  get <- function() {

    m
  }
  

  setInverse <- function(inverse) {
    i <<- inverse
  }

  getInverse <- function() {

    i
  }